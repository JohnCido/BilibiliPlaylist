{
  "name": "bilibili-playlist",
  "version": "2.0.0",
  "description": "[![Greenkeeper badge](https://badges.greenkeeper.io/JohnCido/BilibiliPlaylist.svg)](https://greenkeeper.io/) [![Build Status](https://travis-ci.org/JohnCido/BilibiliPlaylist.svg?branch=master)](https://travis-ci.org/JohnCido/BilibiliPlaylist)",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnCido/BilibiliPlaylist.git"
  },
  "bugs": {
    "url": "https://github.com/JohnCido/BilibiliPlaylist/issues"
  },
  "homepage": "https://github.com/JohnCido/BilibiliPlaylist#readme",
  "scripts": {
    "start": "webpack --env.NODE_ENV=development -w",
    "dev:chrome": "webextension-toolbox dev chrome -s webpackout -t development/[vendor]",
    "dev:firefox": "webextension-toolbox dev firefox -s webpackout -t development/[vendor]",
    "dev:opera": "webextension-toolbox dev opera -s webpackout -t development/[vendor]",
    "dev:edge": "webextension-toolbox dev edge -s webpackout -t development/[vendor]",
    "production": "webpack --env.NODE_ENV=production",
    "distributeUnix": "rm -rf production && npm run production && cd production; zip -r -X upload.zip *",
    "licenses:json": "npm-license-crawler --onlyDirectDependencies --omitVersion --json ./licenses.json"
  },
  "keywords": [
    "chrome",
    "extension"
  ],
  "author": "John Cido",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/amplitude-js": "^4.4.0",
    "@types/async": "^2.0.50",
    "@types/seededshuffle": "^0.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "puppeteer-core": "^1.11.0",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.21.0",
    "terser-webpack-plugin": "^1.2.1",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.1",
    "vue-template-compiler": "^2.5.16",
    "web-ext": "^2.9.3",
